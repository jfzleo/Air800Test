@import "../common/common.wxss";

/**index.wxss**/
.container {
  flex-direction: column;
  box-sizing: border-box;
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  align-content: center;
  padding: 0rpx 0rpx;
}

button{
  margin-top: 30rpx;
  margin-bottom: 50rpx;
}

.navigator {
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  width: 400rpx;
  padding: 50rpx 30rpx;
}

.navigator-box-show {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  flex-direction: column;
  flex-grow: 5;
  margin-bottom: auto;
}
.app-info {
  position: relative;
  flex-grow:1;
  display:flex;
  justify-content: center;
  align-items: center;
  align-self: center;
  flex-direction: column;
  margin-top: auto;
}
text {
  padding:3rpx 0rpx;
  font-size: 24rpx;
}